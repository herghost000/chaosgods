{
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "lint": "eslint . --fix",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "ui:color": "esno ./scripts/gen-colors.ts",
    "ui:css": "esno ./scripts/style-bundle.ts",
    "test:unit": "vitest",
    "coverage": "vitest --coverage",
    "clean": "rimraf packages/*/node_modules && rimraf node_modules",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "pnpm changeset:release:only",
    "changeset:release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "changeset:pre:alpha": "changeset pre enter alpha",
    "changeset:pre:beta": "changeset pre enter beta",
    "changeset:pre:rc": "changeset pre enter rc",
    "changeset:pre:exit": "changeset pre exit",
    "commit": "cz"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@ant-design/colors": "^7.0.2",
    "@antfu/eslint-config": "^2.8.3",
    "@changesets/cli": "^2.27.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/less": "^3.0.6",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.36",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-vitepress-demo": "^2.2.1",
    "vitepress": "1.0.0-rc.45",
    "vitest": "^1.4.0",
    "vue-tsc": "^1.8.27",
    "zx": "^7.2.3"
  },
  "lint-staged": {
    "./**/*.{ts,json,vue,md,jsx,tsx,js,css,less}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
