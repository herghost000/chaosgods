{
  "name": "chaosgods",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "lint": "eslint . --fix",
    "build": "pnpm -r -F !cli -F !utils run build",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "docs:vercel": "cross-env BUILD_VERCEL=true vitepress build",
    "ui:color": "esno ./scripts/gen-colors.ts",
    "ui:css": "esno ./scripts/style-bundle.ts",
    "test:unit": "vitest run",
    "coverage": "vitest run --coverage",
    "clean": "rimraf packages/*/node_modules && rimraf node_modules",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "pnpm changeset:release:only",
    "changeset:release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "changeset:pre:alpha": "changeset pre enter alpha",
    "changeset:pre:beta": "changeset pre enter beta",
    "changeset:pre:rc": "changeset pre enter rc",
    "changeset:pre:exit": "changeset pre exit",
    "commit": "cz"
  },
  "dependencies": {
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@ant-design/colors": "^7.0.2",
    "@antfu/eslint-config": "^2.17.0",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@mdi/font": "6.2.95",
    "@mdi/js": "6.2.95",
    "@mdi/svg": "6.2.95",
    "@tsconfig/node20": "^20.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/less": "^3.0.6",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^28.1.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.21.2",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-serializer-html": "^7.1.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "sass": "^1.77.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vitepress-demo": "^2.2.1",
    "vite-plugin-warmup": "^0.1.0",
    "vitepress": "1.1.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16",
    "zx": "^8.0.2"
  },
  "lint-staged": {
    "./**/*.{ts,json,vue,md,jsx,tsx,js,css,less}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
